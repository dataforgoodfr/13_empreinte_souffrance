    "physical_psychological_pain": {
      "elements": [
        {
          "element_type": "text",
          "text_element": {
            "html": "\u003Cb\u003ELa douleur physique\u003C/b\u003E comprend toutes les souffrances corporelles subies par les animaux : fractures, blessures, maladies, difficultés respiratoires, etc."
          }
        },
        {
          "element_type": "text",
          "text_element": {
            "html": "\u003Cb\u003ELa douleur psychologique\u003C/b\u003E comprend les souffrances mentales vécues par les animaux : stress, anxiété, impossibilité d'exprimer les comportements naturels, etc."
          }
        }

      ],
      "level": "info",
      "title_element": {
          "title": "Les souffrances physiques et psychologiques",
